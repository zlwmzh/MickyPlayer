<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent" tools:context="com.micky.www.videomodule.MainActivity">
    <com.micky.www.micplayerlibrary.MickyPlayerView
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:id="@+id/mickyplayerview"></com.micky.www.micplayerlibrary.MickyPlayerView>
    <SurfaceView
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_marginTop="10dp"
        android:id="@+id/surfaceview"
        />

</LinearLayout>
